<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Dive Sites - Bhaccasyoniztas Beach Resort Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Rooms</h1>
							<ul id="rooms">
			{{#each rooms}}
								<li>
									<a href="/rooms"><img src="images/{{this.image}}" alt="Img"></a>
									<h2><a href="rooms">{{this.name}}</a></h2>
									{{{this.description}}}
									<span class="rate">Rate: {{this.rate}} / Day</span>
			{{/each}}
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>